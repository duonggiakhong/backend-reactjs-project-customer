<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Document</title>
        <link rel="stylesheet" href="/css/styles.css">
        <link
            href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
            rel="stylesheet"
            integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
            crossorigin="anonymous">
        <script
            src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
            crossorigin="anonymous"></script>
    </head>
    <body>
        <div class="topnav">
            <a class="active" href="#home">Home</a>
            <a href="/create">User Name</a>
        </div>

        <div class="text-center">
            world dear. send goodluck
        </div>

        <table class="table table-hover">
            <thead>
                <tr class="table-warning">
                    <th scope="col">ID</th>
                    <th scope="col">Email</th>
                    <th scope="col">Name</th>
                    <th scope="col">City</th>
                    <th scope="col" class="text-center"> Action</th>
                </tr>
            </thead>
            <tbody>
                <% listDataUsers.forEach(function(users) { %>
                <tr>
                    <th scope="row"><%= users.id %></th>
                    <td><%= users.email %></td>
                    <td><%= users.name %></td>
                    <td><%= users.city %></td>
                    <td class="text-center"><button><a
                                href="/editName/<%= users.id %>">Edit</a></button>
                        <button><a
                                href="/delete/<%= users.id %>">Delete</a></button></td>
                </tr>
                <% }) %>
            </tbody>
        </table>

        <!-- my pet here -->
        <div id="pot">
            <img src="https://i.stack.imgur.com/qgNyF.png?s=328&g=1" width=100px
                height=100px>
        </div>
    </body>
</html>